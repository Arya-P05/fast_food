{
  "expo": {
    "name": "fast_food", // the name of the app as it will appear in the app store and on phones
    "slug": "fast_food", // unique identifier for the app and used to build the url if running on the web
    "version": "1.0.0", // the version of the app
    "orientation": "portrait", // the default orientation of the app
    "icon": "./assets/images/icon.png", // the icon of the app
    "scheme": "fastfood", // the scheme of the app to enable deep linking
    "userInterfaceStyle": "automatic", // light, dark or automatic
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true // can be used on ipad or not
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
